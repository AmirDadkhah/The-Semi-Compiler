#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char temp[50][100];

int i,j,x,k;

int main()
{
	char *s;
	FILE *fp,*fp1;
	s=(char *)malloc(1000000);
	fp=fopen("Note.txt","r+");
	
	for(i=0;i<=49;i++){
		k=i-1;
		if(isspace(temp[i][0])!=0){
			break;
		}		
		fgets(temp[i],99,fp);


	}
	
	for(x=0;x<=k;x++){
		for(j=0;j<=99;j++){
			printf("%c",temp[x][j]);
		}
		printf("\n");
	}
	
	fp1=fopen("Note1.txt","r+");
	for(x=0;x<=k;x++){
		fprintf(fp1,"\n");
		for(j=0;j<=99;j++){
			fprintf(fp1,"%c",temp[x][j]);
		}
	}
	fclose(fp);
	fclose(fp1);
	return 0;
}
