#include <stdio.h>
#include <string.h>

int main()
{
	FILE *fp;
	char s[200];
	char *TokenPtr,*temp;
	fp=fopen("Note1.txt","r+");
	fgets(s,200,fp);
	printf("%s",s);
	TokenPtr=strtok(s," ");
	while(TokenPtr != NULL){
		temp=TokenPtr;
		printf("\n%s",temp);
		TokenPtr=strtok(NULL," ");
	}
	fclose(fp);
	return 0;
}
